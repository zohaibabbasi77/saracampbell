{
  "components/common/quick-search": "<div>\n    {{{ snippet \"forms_search\"}}}\n    <form class=\"form\" action=\"{{urls.search}}\">\n        <fieldset class=\"form-fieldset\">\n            <div class=\"form-field\">\n                <label class=\"is-srOnly\" for=\"search_query\">{{lang \"search.quick_search.input_label\"}}</label>\n                <input class=\"search\" type=text data-search-quick name=\"search_query\" id=\"search_query\" data-error-message=\"{{lang 'search.error.empty_field'}}\" placeholder=\"{{lang 'Search'}}\" autocomplete=\"off\">\n                <button type=\"submit\" class=\"form-button\">\n                    <i class=\"icon\" aria-hidden=\"true\">\n                        <svg><use xlink:href=\"#icon-search\" /></svg>\n                    </i>\n                </button>\n            </div>\n        </fieldset>\n    </form>\n    <section class=\"quickSearchResults\" data-bind=\"html: results\"></section>\n</div>\n"
}
